{
  "name": "parcel-app",
  "version": "0.1.0",
  "private": true,
  "main": "main.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "electron .",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "format": "prettier .  --write",

    "clean": "rm -rf ./build",
    "clean:osx": "rm -rf ./build/osx",
    "clean:win": "rm -rf ./build/win",
    "clean:linux": "rm -rf ./build/linux",
    "pack": "npm run clean && npm run pack:osx && npm run pack:win32 && npm run pack:win && npm run pack:linux",
    "pack:osx": "npm run clean:osx && electron-packager . \"ExPOS\" --out=build/osx --platform=darwin --arch=x64 --ignore=nbproject --ignore=node_modules --ignore=build --ignore=src --ignore=builder.json --ignore=README.md --ignore=.gitignore",
    "pack:win32": "npm run clean:win && electron-packager . \"ExPOS\" --out=build/win32 --platform=win32 --arch=ia32 --ignore=nbproject --ignore=node_modules --ignore=build --ignore=src --ignore=builder.json --ignore=README.md --ignore=.gitignore",
    "pack:win": "npm run clean:win && electron-packager . \"ExPOS\" --out=build/win --platform=win32 --arch=x64 --ignore=nbproject --ignore=node_modules --ignore=build --ignore=src --ignore=builder.json --ignore=README.md --ignore=.gitignore",
    "pack:linux": "npm run clean:linux && electron-packager . \"ExPOS\" --out=build/linux --platform=linux --arch=x64 --ignore=nbproject --ignore=node_modules --ignore=build --ignore=src --ignore=builder.json --ignore=README.md --ignore=.gitignore"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@mdi/font": "^7.0.96",
    "@popperjs/core": "^2.11.6",
    "animate.css": "^4.1.1",
    "axios": "^1.2.0",
    "file-saver": "^2.0.5",
    "md5": "^2.3.0",
    "papaparse": "^5.3.2",
    "pouchdb-browser": "^7.3.1",
    "slugify": "^1.6.5",
    "vue": "^3.2.41",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "autoprefixer": "^10.4.13",
    "electron": "^22.0.0",
    "electron-packager": "^17.1.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "postcss": "^8.4.19",
    "prettier": "2.8.0",
    "sass": "^1.56.1",
    "tailwindcss": "^3.2.4",
    "vite": "^3.2.3",
    "vite-plugin-eslint": "^1.8.1"
  }
}
